nodeDepth <- function(tree,node) {
depths <- getDepths(tree)
allDepths <- c(depths$tipDepths,depths$nodeDepths)
l <- length(allDepths)
if (node > l) stop(paste0("Please supply a valid node number between 1 and ",l))
return(allDepths[[node]])
}
